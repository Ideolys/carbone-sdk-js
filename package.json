{
  "name": "carbone-render-sdk-javascript",
  "version": "1.0.0",
  "description": "The javascript SDK to use Carbone Render easily ",
  "homepage": "https://github.com/vuejs/vite/tree/master/#readme",
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && node ./bin/build.js",
    "test": "jest --config jest.config.json --coverage",
    "lint": "eslint \"src/**/*.js\" \"tests/**/*.js\" --fix && prettier --write \"src/**/*.js\" \"tests/**/*.js\""
  },
  "files": [
    "bin",
    "dist/**/*.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "author": "steevepay",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.6.26",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "fetch-mock-jest": "^1.3.0",
    "husky": "^4.2.5",
    "jest": "^26.4.1",
    "lint-staged": "^10.2.11",
    "mocha": "^8.1.1",
    "node-fetch": "^2.6.0",
    "prettier": "2.0.5"
  }
}
