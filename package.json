{
  "name": "carbone-sdk-js",
  "version": "1.1.4",
  "description": "The javascript SDK to use Carbone Render easily ",
  "homepage": "https://github.com/carboneio/carbone-sdk-js",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/carboneio/carbone-sdk-js"
  },
  "scripts": {
    "build": "rm -rf dist && mkdir dist && node ./bin/build.js",
    "test": "jest --config jest.config.json --coverage",
    "lint": "eslint \"src/**/*.js\" \"tests/**/*.js\" --fix && prettier --write \"src/**/*.js\" \"tests/**/*.js\""
  },
  "files": [
    "dist/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "author": "steevepay",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.14.4",
    "core-js": "^3.13.1",
    "esbuild": "^0.6.34",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^23.20.0",
    "fetch-mock-jest": "^1.5.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mocha": "^8.4.0",
    "node-fetch": "^2.6.1",
    "prettier": "2.0.5",
    "regenerator-runtime": "^0.13.7"
  }
}
